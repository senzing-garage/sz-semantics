sz:SenzingScheme a skos:ConceptScheme;
  dcterms:title "Resource"@en;
  skos:definition "A mid-level SKOS taxonomy used for representing Senzing results"@en;
  skos:hasTopConcept sz:DataRecord, sz:Entity, sz:Organization, sz:Person;
  skos:prefLabel "Senzing minimal SKOS taxonomy"@en;
  dc:publisher <https://senzing.com/>;
  dc:creator <https://orcid.org/0000-0003-3167-1539> .

sz:DataRecord a skos:Concept, prov:Entity, wco:TransactionalEvent;
  skos:definition "Data Record is a composite data type; an information block containing specific, identified fields"@en;
  skos:inScheme sz:SenzingScheme;
  skos:topConceptOf sz:SenzingScheme;
  skos:prefLabel "Data Record"@en;
  skos:altLabel "Record"@en;
  sz:lemma_phrase "NOUN.data NOUN.record"@en;
  dc:identifier wd:Q2470517 .

sz:Entity a skos:Concept, bods:Entity, ftm:LegalEntity, wco:Agent;
  skos:definition "Entity is anything that can be considered, discussed, or observed; in this context, this is generally a Person or Organization"@en;
  skos:inScheme sz:SenzingScheme;
  skos:topConceptOf sz:SenzingScheme;
  skos:prefLabel "Entity"@en;
  sz:lemma_phrase "NOUN.entity"@en;
  dc:identifier wd:Q35120 .

sz:Organization a skos:Concept, ftm:Organization, wco:Organization;
  rdfs:subClassOf sz:Entity;
  skos:definition "Organization is a social entity established to meet needs or pursue goals"@en;
  skos:inScheme sz:SenzingScheme;
  skos:topConceptOf sz:SenzingScheme;
  skos:prefLabel "Organization"@en;
  sz:lemma_phrase "NOUN.organization"@en;
  sz:ner_label true;
  dc:identifier wd:Q43229 .

sz:Person a skos:Concept, bods:Person, ftm:Person, wco:Person;
  rdfs:subClassOf sz:Entity;
  skos:definition "Person is any single member of Homo sapiens, unique extant species of the genus Homo"@en;
  skos:inScheme sz:SenzingScheme;
  skos:topConceptOf sz:SenzingScheme;
  skos:prefLabel "Person"@en;
  sz:lemma_phrase "NOUN.person"@en;
  sz:ner_label true;
  dc:identifier wd:Q5 .

sz:lemma_phrase rdfs:domain skos:Concept;
  rdfs:range rdf:langString;
  skos:definition "the parsed, lemmatized noun phrase for an extracted entity"@en .

sz:match_key rdfs:domain sz:Entity;
  rdfs:range xsd:string;
  skos:definition "which features matched when a record last was processed individually"@en .

sz:match_level rdfs:domain sz:Entity;
  rdfs:range xsd:integer;
  skos:definition "signifies how the source record contributes to this entity"@en .

sz:member_of a sz:relation, wco:memberOf;
  rdfs:domain sz:Person;
  rdfs:range sz:Organization;
  skos:definition "which people are members of which organizations"@en .

sz:ner_label rdfs:domain skos:Concept;
  rdfs:range xsd:boolean;
  skos:definition "whether the skos:prefLabel value can be used as an NER label"@en .
